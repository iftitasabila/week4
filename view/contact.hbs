<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Bootstrap demo</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  <link rel="stylesheet" href="../assets/contact.css" />
  <style>
    .active {
      color: orangered !important;
    }
  </style>
</head>

<body>

  <!-- Navbar -->
  <nav class="navbar navbar-expand-lg bg-body-tertiary">
    <div class="container-fluid">
      <a class="navbar-brand" href="/">
        <img class="logo" src="./assets/dumbways.png" alt="logo-image" style="width: 50px; " />
      </a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <li class="nav-item">
            <a class="nav-link " aria-current="page" href="/">Home</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/project">My Project</a>
          </li>
          <li class="nav-item">
            <a class="nav-link active" href="/testimonials">Testimonials</a>
          </li>
        </ul>
        <a class="btn btn-danger" href="/contact">Contact Me</a>
        </form>
      </div>
    </div>
  </nav>
  <!-- Nabar End -->


  <!-- Formulir-->
  <form> <!--autocomplete="off"-->
    <h1 style="text-align: center;"> Get In Touch</h1>
    <div>
      <label for="nameInput">Name</label>
      <input id="nameInput" type="text" required placeholder="input your name" />
    </div>

    <div>
      <label for="emailInput">Email</label>
      <input id="emailInput" type="email" />
    </div>

    <div>
      <label for="phoneNumberInput">Phone Number</label>
      <input id="phoneNumberInput" type="number" />
    </div>

    <div>
      <label for="subject">Subject</label>
      <select id="subject">
        <option hidden>Select Subject</option>
        <option>fullstack Developer</option>
        <option>FrondEnd Developer</option>
        <option>BackEnd Developer</option>
      </select>
    </div>

    <div>
      <label for="address">Address</label>
      <textarea id="address"></textarea>
    </div>

    <div>
      <button class="btn btn-danger" type="button" onclick="getData()">Submit</button>
    </div>
  </form>
  <!-- Formulir End-->

  <script src="../assets/contact.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
    crossorigin="anonymous"></script>
</body>

</html>